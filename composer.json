{
    "name": "tarek111/laravel-unique-slug",
    "description": "A unique slug generator for laravel",
    "keywords": [
        "laravel_unique_slug",
        "laravel",
        "laravel-unique-slug"
    ],
    "homepage": "https://github.com/myoltarek/laravel-unique-slug",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tarek\\LaravelUniqueSlug\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tarek"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarek\\LaravelUniqueSlug\\UniqueSlugServiceProvider"
            ],
            "aliases": {
                "UniqueSlug": "Tarek\\LaravelUniqueSlug\\UniqueSlug"
            }
        }
    }
}
